# My lame git shortcut
alias gh='git status .'

# Open this file in visual studio code editor at line 100, approximately where our aliases are
alias aliases='code -g ~/.zshrc:100'

# Map open to finder
alias finder='open $1'

# Vagrant and VVV
alias vv='cd ~/vagrant-local'
alias vlog='tail -c 25000 -f ~/vagrant-local/log/php7.0_errors.log'